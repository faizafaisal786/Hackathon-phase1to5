# ==============================================
# FREE TIER CONFIGURATION
# ==============================================
# All services below are 100% FREE

# Upstash Kafka (FREE - 10,000 messages/day)
# Get from: https://console.upstash.com/kafka
UPSTASH_KAFKA_URL=https://YOUR_CLUSTER.upstash.io
UPSTASH_KAFKA_USERNAME=YOUR_USERNAME
UPSTASH_KAFKA_PASSWORD=YOUR_PASSWORD

# Upstash Redis (FREE - 10,000 commands/day)
# Get from: https://console.upstash.com/redis
UPSTASH_REDIS_URL=redis://default:YOUR_PASSWORD@YOUR_HOST.upstash.io:6379

# Event System
EVENTS_ENABLED=true

# OpenAI (Optional - use "demo" for testing without API)
OPENAI_API_KEY=demo

# App Config
DEBUG=false
